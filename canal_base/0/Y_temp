FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      ${MOLECULE};
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform ${INITIAL_CONCENTRATION};

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           uniform ${INITIAL_CONCENTRATION};
    }
    outlet
    {
        type            zeroGradient;
    }
    symmetry
    {
        type            symmetryPlane;
    }
    walls
    {
        type            zeroGradient;
    }
    front
    {
        type            symmetryPlane;
    }
    back
    {
        type            zeroGradient;
    }
}


