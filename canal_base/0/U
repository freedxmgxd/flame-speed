FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           uniform ${velocity_inlet};
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           $internalField;
    }
    canal_wall
    {
        type            noSlip;
    }
    plane_symmetry_z
    {
        type            symmetryPlane;
    }
    plane_symmetry_y
    {
        type            symmetryPlane;
    }
}


// ************************************************************************* //
