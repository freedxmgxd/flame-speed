FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 2.0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    0.0002;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      $internalField;
    }
    canal_wall
    {
        type            omegaWallFunction;
        value           $internalField;
    }
    plane_symmetry_z
    {
        type            symmetryPlane;
    }
    plane_symmetry_y
    {
        type            symmetryPlane;
    }
}


// ************************************************************************* //
